/**/#include <avr/wdt.h>char INPUTS[19]; enum INPUT_FIELDS {  INPUT_A,  INPUT_B,  INPUT_X,  INPUT_Y,  INPUT_RB,  INPUT_LB,  INPUT_MENU,  INPUT_LT,  INPUT_RT,  INPUT_UP,  INPUT_RIGHT,  INPUT_DOWN,  INPUT_LEFT,  INPUT_LS,  INPUT_RS,  INPUT_LX,  INPUT_LY,  INPUT_RX,  INPUT_RY};void setup() {  wdt_enable(WDTO_2S);  Serial.begin(9600);  pinMode(LED_BUILTIN, OUTPUT);  digitalWrite(LED_BUILTIN, LOW);}void loop() {  if (Serial.available() >= 19) {    Serial.readBytes(INPUTS, 19);    wdt_reset();  }    if (INPUTS[INPUT_A] == char(1)) {    digitalWrite(LED_BUILTIN, HIGH);    } else {    digitalWrite(LED_BUILTIN, LOW);  }}